{
  "name": "boilerplate-react-webpack",
  "version": "0.2.3",
  "description": "A webpack React starter",
  "repository": "https://github.com/dev-academy-challenges/boilerplate-react-webpack.git",
  "main": "server/index.js",
  "author": "EDA",
  "license": "ISC",
  "scripts": {
    "client": "webpack serve --config ./client/webpack.config.js --hot",
    "dev": "run-p dev:client dev:server",
    "dev:client": "npm run webpack -- --watch",
    "dev:server": "nodemon server",
    "start": "node server",
    "build": "run-p build:client build:server",
    "build:client": "npm run webpack -- --mode production",
    "build:server": "npm run knex -- migrate:latest",
    "server": "node server",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "webpack": "webpack --config ./client/webpack.config.js",
    "knex": "knex --knexfile ./server/db/knexfile.js",
    "lint": "eslint --ext .js,.jsx .",
    "h:deploy": "git push heroku main",
    "h:migrate": "heroku run npm run knex migrate:latest",
    "h:rollback": "heroku run npm run knex migrate:rollback",
    "h:seed": "heroku run npm run knex seed:run"
  },
  "keywords": [
    "fullstack"
  ],
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-object-rest-spread"
    ]
  },
  "eslintConfig": {
    "extends": "eda",
    "ignorePatterns": [
      "bundle.js"
    ]
  },
  "jest": {
    "setupFiles": [
      "./tests/setup-dotenv.js",
      "./tests/client/setup-enzyme.js"
    ]
  },
  "dependencies": {
    "authenticare": "^0.4.8",
    "bcrypt": "^5.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "knex": "^0.21.17",
    "pg": "^8.5.1",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "heroku-cli": "^7.0.9",
    "superagent": "^6.1.0",
    "bulma": "^0.9.2",
    "react-bulma-components": "^3.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.0.1",
    "dotenv-webpack": "^6.0.0",
    "eslint": "7.9.0",
    "eslint-config-eda": "0.1.4",
    "jest": "^26.6.3",
    "jsdom": "^16.4.0",
    "mini-css-extract-plugin": "^1.3.1",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "sass": "^1.29.0",
    "sass-loader": "^10.1.0",
    "supertest": "^5.0.0",
    "sqlite3": "^4.2.0",
    "style-loader": "^2.0.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.17.0",
    "webpack-cli": "^4.4.0",
    "webpack-dev-server": "^3.11.2"
  }
}
